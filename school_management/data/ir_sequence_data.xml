<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequence for sale-->
        <record id="exam_details_sequence_id" model="ir.sequence">
            <field name="name">Exams Sequence</field>
            <field name="code">exam.details</field>
            <field name="prefix">E</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>
        <!-- Sequence for student -->
        <record id="student_details_sequenc_id" model="ir.sequence">
            <field name="name">Student Sequence</field>
            <field name="code">student.details</field>
            <field name="prefix">STU_</field>
            <field name="padding">2</field>
            <field name="number_next">1</field>
        </record>
    </data>
</odoo>
